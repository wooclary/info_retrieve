        function ChangeTab(number, TdID, DIvID, Tablen) {

            var len = Tablen
            var controlID = TdID + number;
            var imgsrc = document.getElementById(controlID).background;
            imgsrcdefault = imgsrc.substring(0, imgsrc.lastIndexOf('/') + 1);

            for (var i = 1; i < len; i++) {
                controlID = TdID + i;
                //document.getElementById(controlID).background=imgsrcdefault+ImgID+"1.gif"
                document.getElementById(controlID).className = "STYLE4";
                controlID = DIvID + i;
                document.getElementById(controlID).style.display = "none";
            }
            controlID = TdID + number;
            //document.getElementById(controlID).background=imgsrcdefault+ImgID+"2.gif"
            document.getElementById(controlID).className = "STYLE41";
            controlID = DIvID + number;
            document.getElementById(controlID).style.display = "";

        }
        function ChangeTab1(number, TdID, DIvID, Tablen) {

            var len = Tablen
            var controlID = TdID + number;
            var imgsrc = document.getElementById(controlID).background;
            imgsrcdefault = imgsrc.substring(0, imgsrc.lastIndexOf('/') + 1);

            for (var i = 1; i < len; i++) {
                controlID = TdID + i;
                //document.getElementById(controlID).background=imgsrcdefault+ImgID+"1.gif"
                document.getElementById(controlID).className = "STYLE4";
                controlID = DIvID + i;
                document.getElementById(controlID).style.display = "none";
            }
            controlID = TdID + number;
            //document.getElementById(controlID).background=imgsrcdefault+ImgID+"2.gif"
            document.getElementById(controlID).className = "STYLE42";
            controlID = DIvID + number;
            document.getElementById(controlID).style.display = "";

        }
